    "def medicine_stock_alert(stock):\n",
    "    print(\"Medicine Stock:\", stock)\n",
    "    \n",
    "    # Conditional logic to check stock level\n",
    "    if stock < 10:\n",
    "        print(\"Status: Low Stock Alert\")\n",
    "    else:\n",
    "        print(\"Status: Stock Sufficient\")\n",
    "\n",
    "\n"
   ]
